<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_picture_show_linear"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/black"
    android:orientation="vertical">
    <View
        android:id="@+id/custom_status_bar_background"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <com.skysoft.smart.intranetchat.model.camera.widget.ClipViewLayout
        android:id="@+id/clipViewLayout_picture"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:clipBorderWidth="@dimen/dp_2" />

</LinearLayout>
